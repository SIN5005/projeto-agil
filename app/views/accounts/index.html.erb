<html>
    <body>
        <table>
    <thead>
        <tr>
            <th>Nome</th>
            <th>Saldo</th>
        </tr>
    </thead>
    <tbody>
        <% @contas.each do |account| %>
        <tr>
            <td><%= account.name %></%></td>
            <td><%= account.balance %></%></td>
            <td><a href="/accounts/<%= account.id %>/remove"</a>Excluir</td>
        </tr>
        <% end %>
    </tbody>
</table>
    </body>
</html>